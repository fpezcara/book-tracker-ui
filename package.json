{
  "name": "book-tracker",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^5.3.5",
    "dotenv": "^16.5.0",
    "history": "^5.3.0",
    "js-cookie": "^3.0.5",
    "latest": "^0.2.0",
    "react": "^19.0.0",
    "react-bootstrap": "^2.10.9",
    "react-dom": "^19.1.0",
    "react-router": "^7.5.2",
    "react-scripts": "^5.0.1",
    "styled-components": "^6.1.17",
    "styled-icons": "^10.35.0",
    "vercel": "^41.6.1"
  },
  "scripts": {
    "e2e": "cypress open --e2e --browser chrome",
    "cy:open": "cypress open",
    "cy:run": "npx cypress run  --spec 'cypress/e2e'",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "pw:run": "npx playwright test --workers=1",
    "pw:open": "npx playwright test --headed --project=chromium --ui",
    "eject": "react-scripts eject",
    "prepare": "husky",
    "format:fix": "prettier --write .",
    "format": "prettier --check .",
    "pact:test": "rm -f pact/pacts/* && REACT_APP_BOOK_TRACKER_API='http://localhost:4015' jest --runInBand --testPathPattern 'test/pact' --testPathIgnorePatterns=[''] && prettier --write 'pact/pacts/*'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.26.9",
    "@pact-foundation/pact": "^15.0.1",
    "@playwright/test": "^1.52.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "babel-jest": "^29.7.0",
    "check-outdated": "^2.13.0",
    "cypress": "^14.3.1",
    "eslint": "^9.25.1",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "jest-fetch-mock": "^3.0.3",
    "jest-pact": "^0.11.3",
    "lint-staged": "^15.5.1",
    "node-fetch": "^3.3.2",
    "pact-broker-cli": "^0.3.2",
    "prettier": "3.5.3",
    "util": "^0.12.5",
    "wait-on": "^8.0.3"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
