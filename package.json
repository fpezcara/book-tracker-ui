{
  "name": "book-tracker",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^5.3.8",
    "dotenv": "^17.2.3",
    "history": "^5.3.0",
    "js-cookie": "^3.0.5",
    "latest": "^0.2.0",
    "react": "^19.0.0",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.2.0",
    "react-router": "^7.9.4",
    "react-scripts": "^5.0.1",
    "styled-components": "^6.1.19",
    "styled-icons": "^10.35.0",
    "vercel": "^48.2.9"
  },
  "scripts": {
    "e2e": "cypress open --e2e --browser chrome",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "pw:run": "npx playwright test --workers=1",
    "pw:open": "npx playwright test --headed --project=chromium --ui",
    "eject": "react-scripts eject",
    "prepare": "husky",
    "format:fix": "prettier --write .",
    "format": "prettier --check .",
    "pact:test": "rm -f pact/pacts/* && REACT_APP_BOOK_TRACKER_API='http://localhost:4015' jest --runInBand --testPathPattern 'test/pact' --testPathIgnorePatterns=[''] && prettier --write 'pact/pacts/*'"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.28.3",
    "@pact-foundation/pact": "^15.0.1",
    "@playwright/test": "^1.56.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "babel-jest": "^30.2.0",
    "check-outdated": "^2.13.0",
    "eslint": "^9.38.0",
    "husky": "^9.1.7",
    "jest-fetch-mock": "^3.0.3",
    "jest-pact": "^0.11.4",
    "lint-staged": "^16.2.4",
    "node-fetch": "^3.3.2",
    "pact-broker-cli": "^0.3.2",
    "prettier": "3.6.2",
    "util": "^0.12.5",
    "wait-on": "^9.0.1"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "overrides": {
    "formidable": "^3.5.3"
  }
}
